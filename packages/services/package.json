{
  "name": "@deta/services",
  "private": true,
  "version": "0.0.0",
  "svelte": "src/index.ts",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./src": "./src/index.ts",
    "./src/*": "./src/*",
