  "files": [
    "build",
    "src/"
  ],
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "@deta/types": "*",
    "svelte": "^5.38.1",
    "@deta/utils": "*",
    "@deta/web-parser": "*",
    "@deta/backend": "*",
    "@horizon/eslint-config": "*",
    "@horizon/typescript-config": "*",
    "typescript": "^5.5.0"
  }
}
