{
  "name": "@deta/services",
  "private": true,
  "version": "0.0.0",
  "svelte": "src/index.ts",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ipc": {
      "types": "./dist/ipc/index.d.ts",
      "svelte": "./src/ipc/index.ts",
      "import": "./dist/ipc/index.js",
      "default": "./dist/ipc/index.js"
    },
    "./views": {
      "types": "./dist/views/index.d.ts",
      "svelte": "./src/views/index.ts",
      "import": "./dist/views/index.js",
      "default": "./dist/views/index.js"
    },
    "./tabs": {
      "types": "./dist/tabs/index.d.ts",
      "svelte": "./src/tabs/index.ts",
      "import": "./dist/tabs/index.js",
      "default": "./dist/tabs/index.js"
    },
    "./shortcuts": {
      "types": "./dist/shortcuts/index.d.ts",
      "svelte": "./src/shortcuts/index.ts",
      "import": "./dist/shortcuts/index.js",
      "default": "./dist/shortcuts/index.js"
    },
    "./resources": {
      "types": "./dist/resources/index.d.ts",
      "svelte": "./src/resources/index.ts",
      "import": "./dist/resources/index.js",
      "default": "./dist/resources/index.js"
    },
    "./notebooks": {
      "types": "./dist/notebooks/index.d.ts",
      "svelte": "./src/notebooks/index.ts",
      "import": "./dist/notebooks/index.js",
      "default": "./dist/notebooks/index.js"
    },
    "./src": "./src/index.ts",
    "./src/*": "./src/*",
    "./src/ipc": "./src/ipc/index.ts",
    "./dist": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src/"
  ],
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "@deta/types": "*",
    "svelte": "^5.38.1",
    "@deta/utils": "*",
    "@deta/web-parser": "*",
    "@deta/backend": "*",
    "@horizon/eslint-config": "*",
    "@horizon/typescript-config": "*",
    "typescript": "^5.5.0"
  }
}
